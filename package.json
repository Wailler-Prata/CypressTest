{
  "devDependencies": {
    "cypress": "^12.12.0",
    "cypress-mochawesome-reporter": "^3.5.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0"
  },
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run --headless",
    "cy:run:mochawesome:flag": "npx cypress run --headless --reporter mochawesome",
    "mergeReports:json": "mochawesome-merge cypress/report/mochawesome-report/*.json > cypress/report/output.json",
    "generateMochawesome-report:html": "marge cypress/report/output.json --reportDir ./ --inline",
    "reset:reports": "node deleteReports.js"
  }
}
